<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="g5.printbook.MainFragment">


    <ImageView
        android:id="@+id/swereaView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="60dp"
        app:srcCompat="@drawable/swereaivf_logo" />

    <ImageView
        android:id="@+id/vinnovaView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/swereaView"
        android:paddingTop="60dp"
        app:srcCompat="@drawable/vinnova_logo" />

</LinearLayout>